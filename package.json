{
  "name": "ivugurura",
  "version": "2.0.0",
  "description": "Reformation and revival",
  "private": true,
  "license": "ISC",
  "packageManager": "yarn@1.22.19",
  "engines": {
    "node": ">=20",
    "yarn": ">=1.22.19"
  },
  "scripts": {
    "start": "babel-node server.js",
    "dev": "vite",
    "lint": "eslint .",
    "test": "vitest",
    "eject": "echo 'not needed with Vite'",
    "lint:fix": "eslint --fix .",
    "format:check": "prettier --check './**/*.{js,jsx,ts,tsx,css,md,json}'",
    "format": "prettier --write './**/*.{js,jsx,ts,tsx,css,md,json}'",
    "typecheck": "tsc --noEmit",
    "build": "vite build",
    "build-staging": "vite build --mode staging",
    "build-prod": "npm run build && npm run compress",
    "mv-build": "rm -rf ../ivugururaApi/build && mv build ../ivugururaApi/build",
    "compress": "tar zcvf build.tar.gz build/",
    "un-compress": "tar zxvf build.tar.gz",
    "mv-build-staging": "rm -rf ../ivugururaApi/build-staging && mv build-staging ../ivugururaApi/build-staging",
    "compress-staging": "tar zcvf build-staging.tar.gz build-staging/",
    "un-compress-staging": "tar zxvf build-staging.tar.gz"
  },
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.0",
    "@fontsource/roboto": "^5.2.5",
    "@mui/icons-material": "^5.16.7",
    "@mui/lab": "^5.0.0-alpha.173",
    "@mui/material": "^5.16.7",
    "@mui/x-date-pickers": "^7.14.0",
    "@reduxjs/toolkit": "^2.6.0",
    "axios": "^1.12.2",
    "buffer": "^6.0.3",
    "classnames": "^2.5.1",
    "crypto-js": "^4.2.0",
    "dayjs": "^1.11.13",
    "draft-js": "^0.11.7",
    "framer-motion": "^12.4.10",
    "history": "^5.3.0",
    "html-react-parser": "^5.2.2",
    "i18next": "^24.2.2",
    "i18next-browser-languagedetector": "^8.0.4",
    "i18next-http-backend": "^3.0.2",
    "is-promise": "^4.0.0",
    "jwt-decode": "^4.0.0",
    "material-react-table": "^2.13.1",
    "moment": "^2.30.1",
    "mui-file-input": "^6.0.0",
    "prop-types": "^15.8.1",
    "react": "^18.3.1",
    "react-avatar-editor": "^13.0.2",
    "react-dom": "^18.3.1",
    "react-draft-wysiwyg": "^1.15.0",
    "react-h5-audio-player": "^3.10.0-rc.1",
    "react-helmet-async": "^2.0.5",
    "react-i18next": "^15.4.1",
    "react-pdf": "^10.2.0",
    "react-redux": "^9.2.0",
    "react-router": "^6.26.1",
    "react-router-dom": "^6.26.1",
    "react-share": "^5.2.2",
    "react-toastify": "^11.0.5",
    "react-youtube": "^10.1.0",
    "redux": "^5.0.1",
    "redux-thunk": "^3.1.0",
    "sass": "^1.85.1",
    "socket.io-client": "^4.8.1",
    "suneditor": "^2.47.5",
    "suneditor-react": "^3.6.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.38.0",
    "@types/node": "^24.8.1",
    "@types/react": "^19.2.2",
    "@types/react-dom": "^19.2.2",
    "@typescript-eslint/eslint-plugin": "^8.46.2",
    "@typescript-eslint/parser": "^8.46.2",
    "@vitejs/plugin-react-swc": "^4.1.0",
    "eslint": "^9.38.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^7.0.0",
    "globals": "^16.4.0",
    "prettier": "^3.6.2",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.46.2",
    "vite": "^7.1.12",
    "vitest": "^3.2.4"
  },
  "resolutions": {
    "eslint-plugin-react": "7.37.2"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
