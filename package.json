{
  "name": "ivugurura",
  "version": "2.0.0",
  "description": "Reformation and revival",
  "private": true,
  "license": "ISC",
  "packageManager": "yarn@1.22.19",
  "engines": {
    "node": ">=20",
    "yarn": ">=1.22.19"
  },
  "scripts": {
    "preinstall": "npx only-allow yarn",
    "start": "babel-node server.js",
    "dev": "react-scripts start",
    "lint": "eslint .",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "lint:fix": "eslint --fix .",
    "format": "prettier --write './**/*.{js,jsx,ts,tsx,css,md,json}'",
    "build": "react-scripts build",
    "build-prod": "npm run build && npm run compress",
    "mv-build": "rm -rf ../ivugururaApi/build && mv build ../ivugururaApi/build",
    "compress": "tar zcvf build.tar.gz build/",
    "un-compress": "tar zxvf build.tar.gz",
    "mv-build-staging": "rm -rf ../ivugururaApi/build-staging && mv build ../ivugururaApi/build-staging",
    "compress-staging": "tar zcvf build-staging.tar.gz build-staging/",
    "un-compress-staging": "tar zxvf build-staging.tar.gz"
  },
  "dependencies": {
    "@babel/plugin-proposal-private-property-in-object": "^7.21.11",
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.0",
    "@fontsource/roboto": "^5.2.5",
    "@mui/icons-material": "^5.16.7",
    "@mui/lab": "^5.0.0-alpha.173",
    "@mui/material": "^5.16.7",
    "@mui/x-date-pickers": "^7.14.0",
    "@reduxjs/toolkit": "^2.2.7",
    "axios": "^1.7.5",
    "buffer": "^6.0.3",
    "classnames": "^2.5.1",
    "dayjs": "^1.11.13",
    "draft-js": "^0.11.7",
    "framer-motion": "^11.3.30",
    "history": "^5.3.0",
    "html-react-parser": "^5.2.2",
    "i18next": "^24.2.2",
    "i18next-browser-languagedetector": "^8.0.4",
    "i18next-http-backend": "^3.0.2",
    "is-promise": "^4.0.0",
    "jwt-decode": "^4.0.0",
    "material-react-table": "^2.13.1",
    "moment": "^2.30.1",
    "mui-file-input": "^6.0.0",
    "prop-types": "^15.8.1",
    "react": "^18.3.1",
    "react-avatar-editor": "^13.0.2",
    "react-dom": "^18.3.1",
    "react-draft-wysiwyg": "^1.15.0",
    "react-h5-audio-player": "^3.10.0-rc.1",
    "react-helmet-async": "^2.0.5",
    "react-i18next": "^15.4.1",
    "react-redux": "^9.2.0",
    "react-router": "^6.26.1",
    "react-router-dom": "^6.26.1",
    "react-scripts": "^5.0.1",
    "react-share": "^5.2.2",
    "react-toastify": "^11.0.5",
    "react-youtube": "^10.1.0",
    "redux": "^5.0.1",
    "redux-thunk": "^3.1.0",
    "sass": "^1.77.8",
    "socket.io-client": "^4.7.5",
    "suneditor": "^2.47.0",
    "suneditor-react": "^3.6.1"
  },
  "devDependencies": {
    "eslint": "^8.57.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^9.1.0",
    "eslint-formatter-lsif": "^1.0.3",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jsx-a11y": "^6.9.0",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-react": "^7.37.2",
    "eslint-plugin-react-hooks": "^4.6.2",
    "prettier": "^3.3.3"
  },
  "workspaces": [
    "packages/*"
  ],
  "resolutions": {
    "eslint-plugin-react": "7.37.2"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
