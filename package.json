{
  "name": "ivugurura",
  "version": "2.0.0",
  "description": "Reformation and revival",
  "private": true,
  "license": "ISC",
  "packageManager": "yarn@1.22.19",
  "engines": {
    "node": ">=16",
    "yarn": ">=1.22.19"
  },
  "scripts": {
    "preinstall": "npx only-allow yarn",
    "start": "babel-node server.js",
    "dev": "react-scripts start",
    "lint": "eslint .",
    "lint:fix": "eslint --fix",
    "format": "prettier --write './**/*.{js,jsx,ts,tsx,css,md,json}'",
    "build": "react-scripts build  && yarn mv-build",
    "mv-build": "rm -rf ../ivugururaApi/build && mv build ../ivugururaApi/build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "compress": "tar zcvf build.tar.gz build/",
    "un-compress": "tar zxvf build.tar.gz"
  },
  "dependencies": {
    "@emotion/react": "^11.9.3",
    "@emotion/styled": "^11.9.3",
    "@mui/icons-material": "^5.10.9",
    "@mui/lab": "^5.0.0-alpha.137",
    "@mui/material": "^5.9.2",
    "@redux-devtools/extension": "^3.2.2",
    "@reduxjs/toolkit": "^1.9.5",
    "axios": "^0.27.2",
    "buffer": "^6.0.3",
    "classnames": "^2.3.2",
    "draft-js": "^0.11.7",
    "history": "^5.3.0",
    "html-react-parser": "^4.0.0",
    "i18next": "^21.8.14",
    "is-promise": "^4.0.0",
    "jwt-decode": "^2.2.0",
    "material-react-table": "^1.11.2",
    "moment": "^2.29.4",
    "mui-file-input": "^3.0.2",
    "prop-types": "^15.8.1",
    "react": "^18.2.0",
    "react-avatar-editor": "^13.0.0",
    "react-bootstrap": "^2.9.1",
    "react-dom": "^18.2.0",
    "react-draft-wysiwyg": "^1.15.0",
    "react-h5-audio-player": "^3.8.6",
    "react-helmet": "^6.1.0",
    "react-i18next": "^11.18.1",
    "react-material-ui-carousel": "^3.4.2",
    "react-redux": "^8.0.7",
    "react-router": "^6.3.0",
    "react-router-dom": "^6.4.2",
    "react-scripts": "^5.0.1",
    "react-toastify": "^9.1.3",
    "react-youtube": "^10.1.0",
    "redux": "^4.2.1",
    "redux-promise-middleware": "^6.1.2",
    "redux-thunk": "^2.4.1",
    "sass": "^1.62.1",
    "socket.io-client": "^4.5.1",
    "suneditor": "^2.45.1",
    "suneditor-react": "^3.6.1"
  },
  "devDependencies": {
    "@babel/plugin-proposal-private-property-in-object": "^7.21.11",
    "@faker-js/faker": "^7.6.0",
    "eslint": "^8.25.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-formatter-lsif": "^1.0.3",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsx-a11y": "^6.6.1",
    "eslint-plugin-react": "^7.31.10",
    "eslint-plugin-react-hooks": "^4.6.0"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "workspaces": {
    "packages": [
      "src/*"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
