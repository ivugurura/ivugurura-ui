{
  "name": "ivugurura",
  "version": "2.0.0",
  "description": "Reformation and revival",
  "private": true,
  "engines": {
    "node": ">=14.17.6 <17.0.0",
    "npm": ">=6.14.16 <7.0.0",
    "engineStrict": true
  },
  "scripts": {
    "start": "babel-node  server.js",
    "dev": "react-scripts start",
    "lint": "eslint .",
    "lint:fix": "eslint --fix",
    "format": "prettier --write './**/*.{js,jsx,ts,tsx,css,md,json}'",
    "build": "react-scripts build  && npm run mv-build",
    "mv-build": "rm -rf ../ivugururaApi/build && mv build ../ivugururaApi/build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "compress": "tar zcvf build.tar.gz build/",
    "un-compress": "tar zxvf build.tar.gz"
  },
  "dependencies": {
    "@emotion/react": "^11.9.3",
    "@emotion/styled": "^11.9.3",
    "@material-ui/core": "^4.12.4",
    "@mui-treasury/components": "^1.10.1",
    "@mui-treasury/styles": "^1.13.1",
    "@mui/icons-material": "^5.10.9",
    "@mui/material": "^5.9.2",
    "@mui/styles": "^5.10.9",
    "@redux-devtools/extension": "^3.2.2",
    "axios": "^0.27.2",
    "bootstrap": "^5.2.0",
    "classnames": "^2.3.2",
    "history": "^5.3.0",
    "i18next": "^21.8.14",
    "is-promise": "^4.0.0",
    "jwt-decode": "^2.2.0",
    "moment": "^2.29.4",
    "prop-types": "^15.8.1",
    "react": "^18.2.0",
    "react-bootstrap": "^2.4.0",
    "react-dom": "^18.2.0",
    "react-helmet": "^6.1.0",
    "react-i18next": "^11.18.1",
    "react-redux": "^8.0.2",
    "react-router": "^6.3.0",
    "react-router-dom": "^6.4.2",
    "react-scripts": "^5.0.1",
    "react-toastify": "^9.0.7",
    "redux": "^4.2.0",
    "redux-promise-middleware": "^6.1.2",
    "redux-thunk": "^2.4.1",
    "socket.io-client": "^4.5.1",
    "typescript": "^4.7.4"
  },
  "devDependencies": {
    "eslint": "^8.25.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-formatter-lsif": "^1.0.3",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsx-a11y": "^6.6.1",
    "eslint-plugin-react": "^7.31.10",
    "eslint-plugin-react-hooks": "^4.6.0"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
